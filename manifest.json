{
    "manifest_version": 2,
    "name": "OneVnet (Unofficial)",
    "version": "0.1.0",
    "description": "帮您快速设置OneVnet，畅游网络。",
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "proxy"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "点击启用或关闭OneVnet服务",
        "default_popup": "panel.html"
    }
}
